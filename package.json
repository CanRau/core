{
  "name": "logux-core",
  "version": "0.2.2",
  "description": "Logux log, synchronization tools, and common utilities",
  "keywords": [
    "logux"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/logux-core",
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "nanoevents": "^1.0.5"
  },
  "devDependencies": {
    "clean-publish": "^1.0.7",
    "eslint": "^5.1.0",
    "eslint-ci": "^0.1.1",
    "eslint-config-logux": "^23.0.4",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.4.1",
    "jsdoc": "^3.5.5",
    "lint-staged": "^7.2.0",
    "nanodelay": "^0.1.4",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "ws": "^5.2.2",
    "yaspeller-ci": "^1.0.0"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "spellcheck": "yarn docs && yaspeller-ci *.md docs/*.html",
    "clean": "rimraf docs/ coverage/",
    "lint": "eslint-ci *.js messages/*.js test/{**/,}*.js",
    "docs": "jsdoc -d docs/ *.js",
    "test": "jest --coverage && yarn lint && yarn spellcheck"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.md": "yaspeller-ci",
    "*.js": "eslint"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "eslintConfig": {
    "extends": "eslint-config-logux/browser"
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "excludeFiles": [
      "docs/*.js.html"
    ],
    "dictionary": [
      "CRDT",
      "Logux",
      "MemoryStore",
      "Redux",
      "js",
      "JS",
      "localStorage",
      "docdash",
      "generateId",
      "JSDoc",
      "nanoevents",
      "DevTools",
      "lastAdded",
      "nodeId",
      "Versioning",
      "timestamp",
      "timestamps",
      "getTime",
      "isFirstOlder",
      "firstMeta",
      "secondMeta",
      "lastSent",
      "lastReceived",
      "getLastSynced",
      "setLastSynced",
      "LastSynced",
      "getLastAdded",
      "getLog",
      "nextLog",
      "TestLog",
      "TestTime",
      "changeMeta",
      "diff",
      "removeReason",
      "preadd",
      "byId",
      "Deryabin",
      "Konstantin",
      "Mamaev",
      "Grigoriy",
      "Beziuk",
      "Alexey",
      "Gaziev",
      "SaaS",
      "Amplifr",
      "eachStoreCheck",
      "waitFor",
      "TestPair",
      "SyncError",
      "ServerNode",
      "ServerConnection",
      "rightNode",
      "rightSent",
      "rightEvents",
      "leftNode",
      "leftSent",
      "leftEvents",
      "LocalPair",
      "ClientNode",
      "BrowserConnection",
      "BaseNode",
      "loopback",
      "SemVer",
      "subprotocol",
      "remoteSubprotocol",
      "minProtocol",
      "localProtocol",
      "errorListener",
      "clientError",
      "authCallback",
      "unsubscribe",
      "remoteProtocol",
      "WebSocket",
      "ws",
      "reconnectable",
      "lastId",
      "GMT",
      "Mon",
      "Tue",
      "Thu",
      "Fri"
    ]
  }
}
