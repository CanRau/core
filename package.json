{
  "name": "logux-core",
  "version": "0.2.2",
  "description": "Logux log, synchronization tools, and common utilities",
  "keywords": [
    "logux"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "logux/logux-core",
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "nanoevents": "^1.0.8"
  },
  "devDependencies": {
    "clean-publish": "^1.0.10",
    "eslint": "^5.8.0",
    "eslint-ci": "^0.1.1",
    "eslint-config-logux": "^26.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.27.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "lint-staged": "^8.0.4",
    "nanodelay": "^0.1.4",
    "rimraf": "^2.6.2",
    "ws": "^6.1.0",
    "yaspeller-ci": "^1.0.0"
  },
  "scripts": {
    "spellcheck": "yarn docs && yaspeller-ci *.md docs/*.html",
    "clean": "rimraf docs/ coverage/",
    "lint": "eslint-ci *.js messages/*.js test/{**/,}*.js",
    "docs": "jsdoc -d docs/ *.js",
    "test": "jest --coverage && yarn lint && yarn spellcheck"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "*.md": "yaspeller-ci",
    "*.js": "eslint"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "eslintConfig": {
    "extends": "eslint-config-logux/browser",
    "overrides": [
      {
        "files": [
          "*.test.js"
        ],
        "rules": {
          "jest/expect-expect": "off"
        }
      }
    ]
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "excludeFiles": [
      "docs/*.js.html"
    ],
    "dictionary": [
      "CRDT",
      "Logux",
      "MemoryStore",
      "Redux",
      "js",
      "JS",
      "localStorage",
      "docdash",
      "generateId",
      "JSDoc",
      "nanoevents",
      "DevTools",
      "lastAdded",
      "nodeId",
      "Versioning",
      "timestamp",
      "timestamps",
      "getTime",
      "isFirstOlder",
      "firstMeta",
      "secondMeta",
      "lastSent",
      "lastReceived",
      "getLastSynced",
      "setLastSynced",
      "LastSynced",
      "getLastAdded",
      "getLog",
      "nextLog",
      "TestLog",
      "TestTime",
      "changeMeta",
      "diff",
      "removeReason",
      "preadd",
      "byId",
      "Deryabin",
      "Konstantin",
      "Mamaev",
      "Grigoriy",
      "Beziuk",
      "Alexey",
      "Gaziev",
      "SaaS",
      "Amplifr",
      "eachStoreCheck",
      "waitFor",
      "TestPair",
      "SyncError",
      "ServerNode",
      "ServerConnection",
      "rightNode",
      "rightSent",
      "rightEvents",
      "leftNode",
      "leftSent",
      "leftEvents",
      "LocalPair",
      "ClientNode",
      "WsConnection",
      "BaseNode",
      "loopback",
      "SemVer",
      "subprotocol",
      "remoteSubprotocol",
      "minProtocol",
      "localProtocol",
      "errorListener",
      "clientError",
      "authCallback",
      "unsubscribe",
      "remoteProtocol",
      "WebSocket",
      "WS",
      "ws",
      "reconnectable",
      "lastId",
      "GMT",
      "Mon",
      "Tue",
      "Thu",
      "Fri",
      "Sep",
      "Oct",
      "remoteNodeId",
      "localNodeId",
      "a SyncError",
      "BrowserConnection"
    ]
  }
}
